META_START
    name: bob
    parameters: alice_id
    csockets: 0 -> alice
    epr_sockets: 0 -> alice
META_END

^b0 {type = CL}:
    csocket = assign_cval() : 0

^b1 {type = QC}:
    run_request(vec<>) : epr_gen_1

^b2 {type = CC}:
    m1 = recv_cmsg(csocket)

^b3 {type = CC}:
    m2 = recv_cmsg(csocket)

^b4 {type = QL}:
    run_subroutine(vec<m1; m2>) : bsm_corrections

^b5 {type = QL}:
    run_subroutine(vec<>) : move_qubit

^b6 {type = QC}:
    run_request(vec<>) : epr_gen_2

^b7 {type = QL}:
    vec<p1; p2> = run_subroutine(vec<>) : bsm

^b8 {type = CL}:
    send_cmsg(csocket, p1)
    send_cmsg(csocket, p2)


SUBROUTINE bsm_corrections
    params: m1, m2
    returns: 
    uses: 0
    keeps: 0
    request:
  NETQASM_START
    set C15 0
    set C14 1
    load C0 @input[C15]
    load C1 @input[C14]
    set Q0 0
    set R0 0
    beq C0 R0 8
    rot_z Q0 16 4
    beq C1 R0 10
    rot_x Q0 16 4
    set R15 0
  NETQASM_END

SUBROUTINE bsm
    params: 
    returns: m1, m2
    uses: 0, 1
    keeps:
    request: 
  NETQASM_START
    set Q0 0
    set Q1 1
    rot_y Q0 8 4
    rot_x Q0 16 4
    rot_y Q1 8 4
    crot_x Q0 Q1 8 4
    rot_x Q0 24 4
    rot_y Q0 24 4
    rot_x Q0 8 4
    rot_y Q0 8 4
    rot_x Q0 16 4
    rot_x Q1 24 4
    rot_y Q1 24 4
    rot_y Q0 8 4
    rot_x Q0 16 4
    meas Q0 M0
    meas Q1 M1
    store M0 @output[1]
    store M1 @output[0]
  NETQASM_END

SUBROUTINE move_qubit
    params: 
    returns: 
    uses: 0, 1
    keeps: 1
    request: 
  NETQASM_START
    set Q0 0
    set Q1 1
    init Q1
    rot_y Q0 8 4
    crot_y Q0 Q1 24 4
    rot_x Q0 24 4
    crot_x Q0 Q1 8 4
  NETQASM_END

REQUEST epr_gen_1
  callback_type: wait_all
  callback: 
  return_vars: 
  remote_id: {alice_id}
  epr_socket_id: 0
  num_pairs: 1
  virt_ids: all 0
  timeout: 1000
  fidelity: 1.0
  typ: create_keep
  role: receive

REQUEST epr_gen_2
  callback_type: wait_all
  callback: 
