META_START
    name: alice
    parameters: bob_id
    csockets: 0 -> bob
    epr_sockets: 0 -> bob
META_END

^b0 {type = CL}:
    csocket = assign_cval() : 0

^b1 {type = QL}:
    run_subroutine(vec<>) : prepare_qubit

^b2 {type = QC}:
    run_request(vec<>) : epr_gen

^b3 {type = QL}:
    vec<m1; m2> = run_subroutine(vec<>) : bsm

^b4 {type = CL}:
    send_cmsg(csocket, m1)
    send_cmsg(csocket, m2)



SUBROUTINE prepare_qubit
    params: 
    returns: 
    uses: 0
    keeps: 0
    request: 
  NETQASM_START
    set Q0 0
    init Q0
    x Q0
  NETQASM_END

SUBROUTINE bsm
    params: 
    returns: m1, m2
    uses: 0, 1
    keeps:
    request: 
  NETQASM_START
    set Q0 0
    set Q1 1
    cnot Q0 Q1
    h Q0
    meas Q0 M0
    meas Q1 M1
    store M0 @output[0]
    store M1 @output[1]
  NETQASM_END

REQUEST epr_gen
  callback_type: wait_all
  callback: 
  return_vars: 
  remote_id: {bob_id}
  epr_socket_id: 0
  num_pairs: 1
  virt_ids: all 1
  timeout: 1000
  fidelity: 1.0
  typ: create_keep
  role: create
