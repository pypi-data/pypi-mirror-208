#!/usr/bin/env python

import os
import sys
from pathlib import Path

import django
from django.core.management import execute_from_command_line


def main():
    sys.path.append(Path.cwd() / 'kirui_devserver')
    os.environ.setdefault("DJANGO_SETTINGS_MODULE", "kirui_devserver.server.settings")
    django.setup()

    execute_from_command_line(sys.argv)


if __name__ == '__main__':
    main()
