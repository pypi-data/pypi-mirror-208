<kr-table class="table" prevPage="" actPage="1" nextPage="2" style="margin-top: 30px;" xmlns:c="https://doculabs.io/2020/xtmpl#control" xmlns:b="https://doculabs.io/2020/xtmpl#data-binding">
    <kr-columns>
        <kr-column id="first" style="width: 500px;">Első</kr-column>
        <kr-column id="second">Második</kr-column>
    </kr-columns>

    <kr-rows>
        <kr-row c:for="row in data">
            <kr-cell b:text="row.first"></kr-cell>
            <kr-cell b:text="row.second"></kr-cell>
        </kr-row>
    </kr-rows>
</kr-table>